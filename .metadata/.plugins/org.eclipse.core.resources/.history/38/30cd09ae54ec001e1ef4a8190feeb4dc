int main(void)
{
	SystemInit();
	DelayMs(1);
	LedOn(LED_GREEN);
	LedOn(LED_ORANGE);
	LedOn(LED_RED);
	LedOn(LED_BLUE);

	while(1) {
		LedOn(LED_BLUE);
		LedON(LED_RED);
		LedOn(LED_ORANGE);
		LedOn(LED_GREEN);
		DelayMs(500);

		LedOf(LED_BLUE);
		LedOf(LED_RED);
		LedOf(LED_ORANGE);
		LedOf(LED_GREEN);
		DelayMs(500);





	}
	return 0;
}
